Algoritmo "semnome"

Var
meta, venda:inteiro
nome:caracter

Inicio
      escreval("Qual o nome do funcionario: ")
      leia (nome)
      escreval("quantas vendas ele fez: ")
      leia (venda)
      
      meta <- 10
       se (venda >= meta)entao
      escreval("Ganhou!")
       senao
      escreval("Não ganhou")
      fimse


Fimalgoritmo